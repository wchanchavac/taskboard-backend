{
	"$schema": "https://json-schema-o8im9w3et-wchanchavac.vercel.app/backend-01.json",
	"project": {
		"name": "Simple Taskboard",
		"description": "This is a simple API from Taskboard"
	},
	"data": [
		{
			"name": "user",
			"attributes": [
				{
					"name": "name",
					"type": "string",
					"required": true
				},
				{
					"name": "email",
					"type": "email",
					"required": true
				},
				{
					"name": "password",
					"type": "string",
					"required": true
				}
			],
			"associations": [
				{
					"model": "board",
					"type": "hasMany",
					"required": true
				}
			]
		},
		{
			"name": "board",
			"attributes": [
				{
					"name": "name",
					"type": "string",
					"required": true
				}
			],
			"associations": [
				{
					"model": "user",
					"type": "belongsTo",
					"required": false
				},
				{
					"model": "list",
					"type": "hasMany",
					"required": false
				}
			]
		},
		{
			"name": "list",
			"attributes": [
				{
					"name": "name",
					"type": "String",
					"required": true
				}
			],
			"associations": [
				{
					"model": "task",
					"type": "hasMany",
					"required": true
				},
				{
					"model": "board",
					"type": "belongsTo",
					"required": true
				}
			]
		},
		{
			"name": "task",
			"attributes": [
				{
					"name": "name",
					"type": "string",
					"required": true
				},
				{
					"name": "status",
					"required": false,
					"type": "enum",
					"defaultValue": "PENDING",
					"values": ["PENDING", "COMPLETE"]
				}
			],
			"associations": [
				{
					"model": "list",
					"type": "belongsTo",
					"required": true
				}
			]
		}
	]
}